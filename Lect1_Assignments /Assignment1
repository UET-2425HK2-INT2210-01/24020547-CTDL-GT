
#include <iostream>
#include <string>
#include <vector>
#include <algorithm>
#include <math.h>
#include <iomanip>
#include <set>

using namespace std;

int main()
{
    std::ios::sync_with_stdio(false);
    cin.tie(NULL);
    string s;
    getline(cin, s);
    for (int i = s.size() - 1; i >= 0; --i) {
        cout << s[i];
    }
}
